{
  "name": "obsidian-snowflake",
  "version": "1.1.0",
  "description": "Automatically adds unique Nano IDs to notes in frontmatter",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc --noEmit && node esbuild.config.mjs production",
    "typecheck": "tsc --noEmit",
    "clean": "rm -f main.js *.tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts --max-warnings 0",
    "lint:fix": "eslint src --ext .ts --max-warnings 0 --fix",
    "lint:watch": "eslint src --ext .ts --max-warnings 0 --watch",
    "prettier": "prettier --check 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "check": "npm run typecheck && npm run test",
    "style": "npm run lint && npm run prettier",
    "style:fix": "npm run lint:fix && npm run prettier:fix"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "nanoid",
    "id",
    "frontmatter"
  ],
  "author": "Ali Yahya",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "typescript-eslint": "^8.38.0"
  }
}
